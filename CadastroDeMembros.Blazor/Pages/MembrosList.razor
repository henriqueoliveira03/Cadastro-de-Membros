@page "/membros"


<PageTitle>Membros</PageTitle>

<h1>Membros</h1>



@if (membros == null)
{
    <p><em>Loading...</em></p>
}
else
{


   


    <MudButton Variant="Variant.Filled" Color="Color.Primary" OnClick="e=>ToggleOpen()">Adicionar</MudButton>
    <table class="table">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nome</th>
                <th>Telefone</th>
                <th>E-mail</th>
                <th>CPF</th>
                <th>Data de Nascimento</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var membro in membros)
            {
                <tr>
                    <td>@membro.ID</td>
                    <td>@membro.Nome</td>
                    <td>@membro.Telefone</td>
                    <td>@membro.Email</td>
                    <td>@membro.CPF</td>
                    <td>@membro.DataDeNascimento</td>
                </tr>
            }
        </tbody>
    </table>
    

              














}
